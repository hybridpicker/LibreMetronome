{
  "name": "frontend",
  "version": "0.6.0",
  "private": true,
  "dependencies": {
    "cra-template": "1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-scripts": "^5.0.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:colors": "react-scripts test ColorScheme.test.js",
    "test:components": "react-scripts test ComponentColorScheme.test.js",
    "test:all-colors": "react-scripts test ColorScheme.test.js ComponentColorScheme.test.js",
    "test:eslint-hooks": "react-scripts test eslint-hooks.test.js",
    "test:eslint-cli": "react-scripts test eslint-cli.test.js",
    "test:lint": "react-scripts test --testMatch=\"**/eslint*.test.js\"",
    "test:build-warnings": "react-scripts test build-warnings.test.js",
    "test:hooks-linting": "react-scripts test hooks-linting.test.js",
    "test:clean-build": "react-scripts test clean-build.test.js",
    "verify-hooks": "npm run test:clean-build",
    "verify-hooks-fix": "./src/tests/verify-hooks-fix.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "jest": "^27.5.1",
    "mock-component": "^5.1.0"
  }
}
